<h1>Log In Homie!!!!</h1>

<%= render "shared/errors" %>
<form action="<%= session_url %>" method="post">
  <%= auth_token %>

  <label>Email:
  <input
    type="text"
    name="user[email]"
  ></label><br>

  <label>Password:
  <input
    type="password"
    name="user[password]"
  ></label><br>

  <button>Submit!</button>
</form>
